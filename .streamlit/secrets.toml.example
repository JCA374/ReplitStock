# Copy this file to secrets.toml and add your API keys
# This file is used by Streamlit Cloud for secrets management

[general]
# Alpha Vantage API Key (optional - app works without it)
ALPHA_VANTAGE_API_KEY = "your_alpha_vantage_key_here"

# Supabase credentials (optional - app works with SQLite)
SUPABASE_URL = "your_supabase_url_here"
SUPABASE_KEY = "your_supabase_anon_key_here"